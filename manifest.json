{
    "manifest_version": 3,
    "name": "UI Comparison Extension",
    "version": "1.0",
    "description": "Compare two URLs for visual differences.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
    "<all_urls>"
  ],
    "background": {
      "service_worker": "background.js"
    }
  }
  